<resources>

    <!-- Base application theme. -->
    <!--设置头部主题，DarkActionBar-->
    <!--取消头部标题，NoActionBar-->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="ownColorbyZD" parent="@style/Widget.AppCompat.Button.Borderless">
        <item name="android:layout_width">120dp</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#19CAAD</item>
        <item name="android:background">@drawable/fillet_boder</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="textSize20sp" >
        <item name="android:textSize">20sp</item>
        <item name="android:padding">10dp</item>
    </style>

    <style name="TextviewLeftStyle" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">20sp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="TextviewshowStyle" >
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">55dp</item>
        <item name="android:textSize">12sp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:maxWidth">150dp</item>
        <item name="android:gravity">center</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_marginRight">@dimen/dp_10</item>
    </style>

    <style name="TextviewRightStyle" >
        <item name="android:layout_width">55dp</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#50000000</item>
        <item name="android:textSize">25sp</item>
        <item name="android:paddingRight">20dp</item>
        <item name="android:layout_gravity">right</item>

    </style>

    <style name="LinearLayoutStyle" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">#FFFFFF</item>
        <item name="android:layout_marginTop">15dp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="MyteamButtonStyle" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:textSize">15sp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:maxWidth">150dp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/selector_bt_bg_compose_blue</item>
    </style>

    <style name="homepage_footer" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:background">#80EED2EE</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center_horizontal</item>
    </style>

    <style name="homepage_footer_tb">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#8a8a8a</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="update_user_info">
        <item name="android:layout_width">100dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="et_update_user_info">
        <item name="android:background">@drawable/tv_boder_gray</item>
        <item name="android:textSize">20sp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">5dp</item>
    </style>

    <style name="layout_update_user_info">
        <item name="android:background">#ffffff</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">10dp</item>
    </style>

    <style name="regist_et_boder">
        <item name="android:layout_width">250dp</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:background">@drawable/fillet_boder</item>
        <item name="android:padding">5dp</item>
    </style>

    <style name="btn_finish_ac">
        <item name="android:layout_width">40dp</item>>
        <item name="android:layout_height">40dp</item>
        <item name="android:background">@drawable/btn_return</item>
        <item name="android:padding">5dp</item>
    </style>
    <style name="head_nav_textview">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">20sp</item>
        <item name="android:layout_marginRight">50dp</item>
        <item name="android:paddingLeft">2dp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="et_boder">
    </style>


</resources>
